package awaybuilder.camera{	import awaybuilder.vo.SceneCameraVO;				public class CameraFactory	{		protected var propertyFactory : CameraPropertyFactory ;								public function CameraFactory ( )		{			this.propertyFactory = new CameraPropertyFactory ( ) ;		}								////////////////////////////////////////////////////////////////////////////////		//		// Public Methods		//		////////////////////////////////////////////////////////////////////////////////								public function build ( vo : SceneCameraVO ) : SceneCameraVO		{			vo = this.propertyFactory.build ( vo ) ;			return vo ;		}	}}