package awaybuilder.collada{	import flash.events.Event;	import flash.net.URLLoader;				public class ColladaLoader extends URLLoader	{		protected var _collada : XML ;								public function ColladaLoader ( )		{			this.addEventListener ( Event.COMPLETE , this.onLoadComplete ) ;		}								////////////////////////////////////////////////////////////////////////////////		//		// Event Handlers		//		////////////////////////////////////////////////////////////////////////////////								protected function onLoadComplete ( event : Event ) : void		{			this.removeEventListener ( Event.COMPLETE , this.onLoadComplete ) ;			this._collada = ColladaUtil.removeNamespaces ( this.data ) ;		}						////////////////////////////////////////////////////////////////////////////////		//		// Getters and Setters		//		////////////////////////////////////////////////////////////////////////////////								public function get collada ( ) : XML		{			return this._collada ;		}	}}